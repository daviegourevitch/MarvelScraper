{
  "name": "marvel-scraper",
  "version": "0.1.0",
  "private": true,
  "description": "A TypeScript scraper that stores data in Neo4j",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "neo4j:up": "docker compose up -d",
    "neo4j:down": "docker compose down",
    "neo4j:logs": "docker compose logs -f | cat"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "neo4j-driver": "^5.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}

