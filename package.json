{
  "name": "marvel-scraper",
  "version": "0.1.0",
  "private": true,
  "description": "A TypeScript scraper that stores data in Neo4j",
  "license": "MIT",
  "scripts": {
    "start": "bun src/index.ts",
    "dev": "bun --hot src/index.ts",
    "up": "bash scripts/start_all.sh",
    "neo4j:up": "docker compose up -d",
    "neo4j:down": "docker compose down",
    "neo4j:logs": "docker compose logs -f | cat"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "hermitpurple": "^0.2.0",
    "neo4j-driver": "^5.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12"
  },
  "packageManager": "bun@1.1.26"
}
